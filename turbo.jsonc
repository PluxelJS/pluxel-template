{
	"$schema": "https://turborepo.com/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"test": {
			"dependsOn": ["build"],
			"outputs": ["dist/**"]
		},
		"pulish": {
			"outputs": ["dist/**"],
			"dependsOn": ["build"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
